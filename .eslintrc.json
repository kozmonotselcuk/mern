{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
			"no-console": "off",
			"quotes": [
				"warn",
				"single"
			],
			"indent":["off", "tab"],
			"no-tabs": "off"
    }
}